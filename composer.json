{
    "name": "byteincoffee/doctrine-extensions",
    "type": "library",
    "description": "FSi Behavioral Doctrine Extensions",
    "keywords": ["fsi", "doctrine-extensions"],
    "license": "MIT",
    "authors": [
        {
            "name": "Lukasz Cybula",
            "email": "lukasz@fsi.pl"
        },
        {
            "name": "Norbert Orzechowicz",
            "email": "norbert@orzechowicz.pl"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "doctrine/common": "~2.3",
        "symfony/property-access": "~2.2",
        "symfony/yaml": "~2.2",
        "fsi/reflection": "0.9.*",
        "fsi/metadata": "0.9.*",
        "fsi/property-observer": "0.9.*",
        "knplabs/gaufrette": "0.1.*"
    },
    "require-dev": {
        "doctrine/dbal": "~2.3",
        "doctrine/orm": "~2.3",
        "phpunit/phpunit": "3.7.*"
    },
    "autoload": {
        "psr-0": {
            "FSi\\DoctrineExtensions": "lib/",
            "FSi\\DoctrineExtensions\\Tests": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.4-dev"
        }
    }
}
